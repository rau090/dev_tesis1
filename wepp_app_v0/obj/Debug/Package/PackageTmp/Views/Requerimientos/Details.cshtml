@model wepp_app_v0.Models.Requerimiento

@{
    ViewBag.Title = "Detalles Requerimiento";
}

<h2>@ViewBag.Title</h2>

<fieldset>
    <legend>Datos</legend>

    <div class="display-label">Num. Requerimiento</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.NumReq)
    </div>

    <div class="display-label">Lider de Proyecto</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.LiderProyecto.ApellidoPaterno) - @Html.DisplayFor(model => model.LiderProyecto.Nombre)
    </div>

    <div class="display-label">Ing. de Soluciones</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.IdS.ApellidoPaterno) - @Html.DisplayFor(model => model.IdS.Nombre)
    </div>

    <div class="display-label">Año Priorizacion</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AnhoPriorizacion)
    </div>
    <div class="display-label">Periodo Priorizacion</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AnhoPriorizacion)
    </div>
    <div class="display-label">Estado</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Estado)
    </div>
    <div class="display-label">Prioridad</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Prioridad)
    </div>

    <div class="display-label">Fecha Priorizacion</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.FechaPriorizacion)
    </div>

    <div class="display-label">Lider Usuario</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.LiderUsuario)
    </div>

    <div class="display-label">Nivel Complejidad</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.NivelComplejidad)
    </div>

    <div class="display-label">Unidad Negocio</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.UnidadNegocio)
    </div>
</fieldset>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.IdRequerimiento }) |
    @Html.ActionLink("Regresar", "Index")
</p>
