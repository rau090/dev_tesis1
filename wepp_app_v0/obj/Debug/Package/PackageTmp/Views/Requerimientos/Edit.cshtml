@model wepp_app_v0.Models.Requerimiento
@{
    ViewBag.Title = "Actualizar Requerimiento";
}
<h2>@ViewBag.Title</h2>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Datos</legend>
        @Html.HiddenFor(model => model.IdRequerimiento)
        @Html.HiddenFor(model => model.IdLiderProyecto)
        @Html.HiddenFor(model => model.IdIdS)
        @Html.HiddenFor(model=>model.Estado)
        <div class="editor-label">
            @Html.LabelFor(model => model.NumReq, "Num. Requerimiento")
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.NumReq)
            @Html.ValidationMessageFor(model => model.NumReq)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.AnhoPriorizacion, "Añor Priorizacion")
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.AnhoPriorizacion)
            @Html.ValidationMessageFor(model => model.AnhoPriorizacion)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.PeriodoPriorizacion, "Periodo Priorizacion")
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.PeriodoPriorizacion)
            @Html.ValidationMessageFor(model => model.PeriodoPriorizacion)
        </div>
        <div class="display-label">
            Lider de Proyecto</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.LiderProyecto.ApellidoPaterno) - @Html.DisplayFor(model => model.LiderProyecto.Nombre)
        </div>
        <div class="display-label">
            Ing. de Soluciones</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.IdS.ApellidoPaterno) - @Html.DisplayFor(model => model.IdS.Nombre)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.Prioridad, "Prioridad")
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Prioridad)
            @Html.ValidationMessageFor(model => model.Prioridad)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.FechaPriorizacion, "Fecha Priorizacion")
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.FechaPriorizacion)
            @Html.ValidationMessageFor(model => model.FechaPriorizacion)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.LiderUsuario, "Lider Usuario")
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.LiderUsuario)
            @Html.ValidationMessageFor(model => model.LiderUsuario)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.NivelComplejidad, "Nivel de Complejidad")
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.NivelComplejidad)
            @Html.ValidationMessageFor(model => model.NivelComplejidad)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.UnidadNegocio, "Unidad de Negocio")
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.UnidadNegocio)
            @Html.ValidationMessageFor(model => model.UnidadNegocio)
        </div>
        <p>
            <input type="submit" value="Grabar" />
        </p>
    </fieldset>
}
<div>
    @Html.ActionLink("Regresar", "Index")
</div>
