@model wepp_app_v0.Models.Vacacion

@{
    ViewBag.Title = "Detalle";
}

<h2>@ViewBag.Title</h2>

<fieldset>
    <legend>Vacacion</legend>


    <div class="display-label">Año</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Anho)
    </div>

    <div class="display-label">Fecha Inicio</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.FechaInicio)
    </div>
    <div class="display-label">Fecha Fin</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.FechaFin)
    </div>

    

    <div class="display-label">Duración</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Periodo)
    </div>

    <div class="display-label">Persola Interno</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.personalInterno.ApellidoPaterno)
    </div>
</fieldset>
<p>
    @Html.ActionLink("Editar", "Edit", new { id=Model.IdVacacion }) |
    @Html.ActionLink("Regresar", "Index")
</p>
