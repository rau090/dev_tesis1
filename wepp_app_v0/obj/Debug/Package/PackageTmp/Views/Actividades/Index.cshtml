@model IEnumerable<wepp_app_v0.Models.Actividad>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            cronograma
        </th>
        <th>
            personalInterno
        </th>
        <th>
            OrdenActividad
        </th>
        <th>
            TipoActividad
        </th>
        <th>
            FechaInicio
        </th>
        <th>
            FechaFin
        </th>
        <th>
            Avance
        </th>
        <th>
            Asignacion
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cronograma.Estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.personalInterno.ApellidoMaterno)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrdenActividad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoActividad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaInicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaFin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Avance)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Asignacion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdActividad }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdActividad }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdActividad })
        </td>
    </tr>
}

</table>
