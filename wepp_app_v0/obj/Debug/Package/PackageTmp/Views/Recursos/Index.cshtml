@model IEnumerable<wepp_app_v0.Models.PersonalInterno>
@{
    ViewBag.Title = "Administrar Recursos";
}
<h2>@ViewBag.Title</h2>
<p>
    @Html.ActionLink("Create Nuevo", "Create")
</p>
<table>
    <tr>
        <th>
            Rol
        </th>
        <th>
            Nombre
        </th>
        <th>
            Apellidos
        </th>
        <th>
            Estado
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Rol)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApellidoPaterno) - @Html.DisplayFor(modelItem => item.ApellidoMaterno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoPersonal)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.IdPersonalInterno }) |
                @Html.ActionLink("Detalle", "Details", new { id = item.IdPersonalInterno })
            </td>
        </tr>
    }
</table>
